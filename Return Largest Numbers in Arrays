function largestOfFour(arr) {
  var result = [];
  for (var n in arr){
    var largestNum = 0;
    for (var i in arr[n]){
      if (arr[n][i]>largestNum)
        largestNum = arr[n][i];
    }
    result[n] = largestNum;
  }
  return result;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
